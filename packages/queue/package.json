{
    "name": "@sooohka/queue",
    "version": "0.0.0",
    "private": true,
    "description": "A native-like Queue implementation with full TypeScript support and Array-like iterators",
    "keywords": [
        "queue",
        "data-structure",
        "fifo",
        "typescript",
        "iterable",
        "native"
    ],
    "license": "MIT",
    "type": "module",
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsup",
        "clean": "git clean -xdf .cache .turbo dist node_modules",
        "dev": "tsup --watch && 'node scripts/dts-as-module.js'",
        "test": "vitest",
        "test:watch": "vitest watch",
        "typecheck": "tsc --noEmit --emitDeclarationOnly false"
    },
    "dependencies": {
        "@microsoft/api-extractor": "^7.53.1",
        "acorn": "^8.15.0",
        "jscodeshift": "^17.3.0",
        "ts-morph": "^27.0.0",
        "tsup": "^8.5.0"
    },
    "devDependencies": {
        "@sooohka/tsconfig": "workspace:*",
        "@types/jscodeshift": "^17.3.0",
        "@types/node": "^24.7.0",
        "vitest": "^3.2.4"
    }
}
