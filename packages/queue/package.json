{
  "name": "@structkit/queue",
  "version": "0.0.2",
  "private": false,
  "description": "A native-like Queue implementation with full TypeScript support and Array-like iterators",
  "repository":{
    "type":"git",
    "url":"git+https://github.com/sooohka/structkit/tree/main/packages/queue"
  },
  "keywords": [
    "queue",
    "data-structure",
    "fifo",
    "typescript",
    "iterable",
    "native"
  ],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.global.js",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsup --watch && 'node scripts/dts-as-module.js'",
    "test": "vitest",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {},
  "devDependencies": {
    "@structkit/tsconfig": "workspace:*",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "^24.7.0",
    "tsup": "^8.5.0"
  }
}
